{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://yourdomain.com/api",
        "EXPO_PUBLIC_SOCKET_URL": "https://yourdomain.com"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://yourdomain.com/api",
        "EXPO_PUBLIC_SOCKET_URL": "https://yourdomain.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
